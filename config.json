{
  "name": "Octo New June",
  "version": "1.0.0",
  "slug": "octo-new-june",
  "description": "A Home Assistant add-on to enable controlling Octo smart beds.",
  "url": "https://github.com/bramboe/octo-june-new",
  "startup": "application",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "services": ["mqtt:need"],
  "host_network": true,
  "rating": 8,
  "ingress": true,
  "panel_icon": "mdi:bed",
  "panel_title": "Octo Bed",
  "options": {
    "mqtt_host": "<auto_detect>",
    "mqtt_port": "<auto_detect>",
    "mqtt_user": "<auto_detect>",
    "mqtt_password": "<auto_detect>",
    "type": "octo",
    "bleProxies": [
      {
        "host": "bluetooth-proxy.local"
      }
    ],
    "octoDevices": [
      {
        "name": "RC2",
        "friendlyName": "Octo Bed",
        "pin": "1234"
      }
    ]
  },
  "schema": {
    "mqtt_host": "str",
    "mqtt_port": "str",
    "mqtt_user": "str",
    "mqtt_password": "password",
    "type": "list(octo)",
    "bleProxies": [
      {
        "host": "str",
        "port": "int(1,65536)?",
        "password": "password?",
        "encryptionKey": "match(^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)$)?",
        "expectedServerName": "str?"
      }
    ],
    "octoDevices": [
      {
        "name": "str",
        "friendlyName": "str",
        "pin": "str?"
      }
    ]
  }
}